<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="TOP_CENTER" prefHeight="495.0" prefWidth="600.0" style="-fx-background-color: #B98CD5;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminRegisterController">
    <children>
        <Label alignment="CENTER" prefHeight="96.0" prefWidth="319.0" text="Create your account">
            <font>
                <Font name="System Bold Italic" size="34.0" />
            </font>
        </Label>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="FirstName" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <TextField fx:id="fname" prefHeight="25.0" prefWidth="215.0" promptText="first name">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </TextField>
                    </children>
                </VBox>
                <VBox alignment="CENTER" layoutX="310.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="LastName" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                  <TextField fx:id="lname" />
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="UserID:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <TextField fx:id="username" prefHeight="25.0" prefWidth="215.0" promptText="userid">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </TextField>
                    </children>
                </VBox>
                <VBox alignment="CENTER" layoutX="310.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Password:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <PasswordField fx:id="password" promptText="password (8 digit)">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </PasswordField>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Phone Number:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <TextField fx:id="phoneno" prefHeight="25.0" prefWidth="215.0" promptText="phone number (11 digit)">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </TextField>
                    </children>
                </VBox>
                <VBox alignment="CENTER" layoutX="310.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Email Address:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <TextField fx:id="email" promptText="Email address">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </TextField>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Date of Birth:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <DatePicker fx:id="dob" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="select from the calender">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </DatePicker>
                    </children>
                </VBox>
                <VBox alignment="CENTER" layoutX="310.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="231.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Gender:" textFill="#bb1111">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                            <font>
                                <Font name="System Bold Italic" size="12.0" />
                            </font>
                        </Label>
                        <ComboBox fx:id="gender" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="Select your gender">
                            <VBox.margin>
                                <Insets left="7.0" right="7.0" />
                            </VBox.margin>
                        </ComboBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button mnemonicParsing="false" onAction="#submit" prefHeight="45.0" prefWidth="170.0" style="-fx-background-color: #5953B6; -fx-background-radius: 7;" text="Submit" textFill="#864747">
                    <font>
                        <Font name="System Bold Italic" size="20.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="15.0" right="15.0" />
                    </HBox.margin>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onAction="#back" opacity="0.84" prefHeight="45.0" prefWidth="170.0" style="-fx-background-color: #C43790; -fx-background-radius: 7;" text="Back" textFill="#3e1010">
                    <font>
                        <Font name="System Bold Italic" size="23.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="15.0" right="15.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
